PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>

DELETE { ?tablerowTor dbo:has_column ?patientIDTor, ?genderTor, ?ageTor, ?hpvTor, ?whostatusTor, ?tstageTor, ?nstageTor, ?mstageTor, ?tumourTor, ?rttotaldaysTor, ?graytotaldoseTor, ?graydoseperfractionTor, ?chemoTor, ?followupdays, ?neoplasmTor, ?radiotherapyTor, ?survivalTor, ?metastasisTor, ?localrecurrenceTor, ?localrecurrencedaysTor, ?regionalrecurrenceTor, ?regionalrecurrencedaysTor, ?metastasisdaysTor.
}

